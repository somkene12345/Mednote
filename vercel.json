{
    "buildCommand": "expo export -p web",
    "outputDirectory": "dist",
    "devCommand": "expo",
    "cleanUrls": true,
    "framework": null,
    "rewrites": [
      {
        "source": "/favicon.ico",
        "destination": "/favicon.ico"
      },
      {
        "source": "/favicon.png",
        "destination": "/favicon.png"
      },
      {
        "source": "/:path*",
        "destination": "/"
      }
    ],
    "headers": [
      {
        "source": "/favicon.ico",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/favicon.png",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }
  